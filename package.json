{
  "name": "zekoder-web-components",
  "version": "0.1.0",
  "private": false,
  "main": "dist/zekoder-web-components.umd.min.js",
  "unpkg": "dist/zekoder-web-components.umd.min.js",
  "jsdelivr": "dist/zekoder-web-components.umd.min.js",
  "author": "Your name",
  "license": "Proprietary",
  "description": "",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run buildBasic && npm run buildMain",
    "buildMain": "vue-cli-service build --target lib --dest dist/main --name zekoder-web-components src/main.js",
    "buildBasic": "vue-cli-service build --target lib --dest dist/basic --name basic-components src/basic-components/main.js",
    "prepublishOnly": "$npm_execpath build",
