  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "core-js": "^3.6.5",
    "docx": "^7.3.0",
    "file-saver": "^2.0.5",
    "fs": "0.0.1-security",
    "graphql": "^16.2.0",
    "graphql-tag": "^2.12.6",
    "js-yaml": "^4.1.0",
    "sanitize-html": "^2.6.1",
    "vue": "^3.0.0",
    "vue-router": "^3.5.3",
    "vue-slick-carousel": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@storybook/addon-actions": "^6.4.9",
